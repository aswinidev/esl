{
  "name": "exadel-smart-library",
  "description": "Exadel Smart Library - is the lightweight custom elements library that provide a set of super-flexible components",
  "keywords": [
    "custom elements",
    "component library"
  ],
  "version": "1.15.2-alpha",
  "private": true,
  "homepage": "https://gitlab.exadel.com/EDMTP/smart-wc-library#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.exadel.com/EDMTP/smart-wc-library.git"
  },
  "main": "./lib/all.js",
  "scripts": {
    "watch": "gulp watch",
    "build": "gulp build",
    "build-granular": "gulp build-granular",
    "local-serv": "npm run build && npm run local-serv-build && npm run local-serv-quick",
    "local-serv-build": "gulp build-local",
    "local-serv-quick": "cd test-server && node server.js",
    "local-serv-watch": "npm run local-serv | npm run watch",
    "prepare": "npm run build -- --fast && npm run build-granular -- --fast"
  },
  "files": [
    "src/**/*",
    "lib/**/*",
    "!lib/**/*.map"
  ],
  "browserslist": [
    "last 3 version",
    "> 1%",
    "not dead"
  ],
  "dependencies": {
    "@types/video.js": "7.3.3",
    "@types/youtube": "0.0.38"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "@webcomponents/custom-elements": "^1.4.1",
    "@webcomponents/webcomponents-platform": "^1.0.1",
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-copy": "^4.0.1",
    "gulp-each": "^0.5.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "intersection-observer": "^0.7.0",
    "promise-polyfill": "^8.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2",
    "ui-project-server": "./test-server/edmtp-modules/ui-project-server.tar",
    "vinyl-named": "^1.1.0",
    "webpack": "^4.42.0",
    "webpack-stream": "^5.2.1"
  }
